[%%SMB_FOLDER_NAME%%]
   comment = %%SMB_COMMENT%%
   path = %%SMB_FOLDER_PATH%%
   read only = yes
   guest ok = yes
   available = %%SMB_FOLDER_AVAIL%%
   force user = admin
   force group = smbshare%%SID%%
   valid users = +smbshare%%SID%% +smbshare%%SID%%ro
   write list = +smbshare%%SID%%
   create mask = 0644
   directory mask = 0755

